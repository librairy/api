{
  "name": "lynx-model",
  "description": "topic model created from lynx dataset",
  "contactEmail": "cbadenes@fi.upm.es",
  "version": "0.1",
  "annotate":false,
  "parameters": {
    "maxdocratio": "0.9",
    "minfreq": "5",
    "multigrams": "false",
    "retries":"0",
    "seed":"1066",
    "alpha": "0.1",
    "topics":"50",
    "beta":"0.01",
    "language": "en",
    "iterations":"1000",
    "lowercase":"true",
    "topwords":"50",
    "autolabels":"true",
    "autowords":"true",
    "stopwords":"",
    "stoplabels":""
  },
  "dataSource": {
    "cache": false,
    "dataFields": {
      "id": "id",
      "text": [
        "txt_t"
      ]
    },
    "filter":"*:*",
    "format": "SOLR_CORE",
    "offset": 0,
    "size": -1,
    "url": "http://librairy.linkeddata.es/solr/lynx"
  }
}
