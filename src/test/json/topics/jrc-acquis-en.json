{
  "name": "jrc-en-model",
  "description": "Collection of legislative texts (EN) from the European Union generated between years 1958 and 2006",
  "contactEmail": "cbadenes@fi.upm.es",
  "version": "1.0",
  "annotate":false,
  "parameters": {
    "maxdocratio": "0.9",
    "minfreq": "5",
    "multigrams": "false",
    "retries":"0",
    "seed":"1066",
    "alpha": "0.1",
    "beta":"0.01",
    "language": "en",
    "iterations":"1000",
    "lowercase":true,
    "topwords":"10",
    "stopwords":"",
    "stoplabels":""
  },
  "dataSource": {
    "cache": false,
    "dataFields": {
      "id": "id",
      "labels": [
        "root-labels_t"
      ],
      "text": [
        "txt_t"
      ]
    },
    "filter":"size_i:[100 TO *] && source_s:jrc && lang_s:en && root-labels_t:[* TO *]",
    "format": "SOLR_CORE",
    "offset": 0,
    "size": 20000,
    "url": "http://librairy.linkeddata.es/solr/jrc"
  }
}