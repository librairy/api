{
  "name": "jrc-acquis-DE-model",
  "description": "Collection of legislative texts (DE) from the European Union generated between years 1958 and 2006",
  "contactEmail": "cbadenes@fi.upm.es",
  "version": "1.1",
  "annotate":false,
  "parameters": {
    "maxdocratio": "0.9",
    "minfreq": "5",
    "multigrams": "false",
    "retries":"0",
    "seed":"1066",
    "alpha": "0.1",
    "beta":"0.01",
    "language": "de",
    "iterations":"1000",
    "stopwords":""
  },
  "dataSource": {
    "cache": false,
    "dataFields": {
      "id": "id",
      "labels": [
        "labels_t"
      ],
      "text": [
        "name_s", "txt_t"
      ]
    },
    "filter":"size_i:[100 TO 10000] && source_s:jrc && lang_s:de && labels_t:[* TO *]",
    "format": "SOLR_CORE",
    "offset": 0,
    "size": -1,
    "url": "http://librairy.linkeddata.es/solr/documents"
  }
}