{
  "name": "ted-de-model",
  "description": "Tenders Electronic Daily (TED) texts (DE) from the European Union",
  "contactEmail": "cbadenes@gmail.com",
  "version": "1.1",
  "annotate":false,
  "parameters": {
    "maxdocratio": "0.9",
    "minfreq": "5",
    "multigrams": "false",
    "retries":"0",
    "seed":"1066",
    "alpha": "0.1",
    "beta":"0.01",
    "language": "de",
    "iterations":"1000",
    "lowercase":"true",
    "topwords":"10",
    "autolabels":"false",
    "autowords":"true",
    "stopwords":"",
    "stoplabels":""
  },
  "docker": {
    "email": "cbadenes@gmail",
    "password": "secret",
    "repository": "librairy/ted-de-model",
    "user": "cbadenes"
  },
  "dataSource": {
    "cache": false,
    "dataFields": {
      "id": "id",
      "labels": [
        "divisions_t"
      ],
      "text": [
        "name_s","txt_t"
      ]
    },
    "filter":"size_i:[100 TO *] && source_s:ted && lang_s:de && labels_t:[* TO *]",
    "format": "SOLR_CORE",
    "offset": 0,
    "size": 10000,
    "url": "http://librairy.linkeddata.es/solr/documents"
  }
}